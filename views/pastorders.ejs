<h2>PRODUCTS LIST</h2>
<br>

<div class="wrap-table100">
    <div class="table100">
        <table>
            <thead>
                <tr class="table100-head">
                    
                    <th class="column2">OID</th>
                    <th class="column3">Date</th>
                    <th class="column2">Bill</th>
                </tr>
            </thead>
            <tbody>
                <% if(userData.length!=0){ %>
                <% var i=1; %>
                <% userData.forEach(function(data){ %>                        
                    <tr>
                        <td class="column2"><%= data.o_id %></td>
                        <td class="column3"><%= data.Date %></td>
                        <td class="column2"><%= data.bill %></td>
                    </tr>
                    
                    <%  i++; }) %>
                    <% } else{ %>
                        <tr>
                            <td colspan="7">No Data Found</td>
                        </tr>
                    <% } %>
            </tbody>
        </table>
    </div>
</div>




